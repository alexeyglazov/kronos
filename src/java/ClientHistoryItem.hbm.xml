<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="ClientHistoryItem" table="client_history_item">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="name" not-null="true"/>
    <property name="codeName" not-null="true"/>
    <property name="createdAt" not-null="true"/>
    <many-to-one class="Client" name="client">
      <column name="client_id" not-null="true"/>
    </many-to-one>
    <many-to-one column="group_id" name="group" not-null="false"/>
    <many-to-one column="created_by_id" name="createdBy" not-null="true"/>
  </class>
</hibernate-mapping>
