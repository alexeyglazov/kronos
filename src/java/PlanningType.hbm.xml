<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="PlanningType" table="planning_type">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="isActive"/>
    <property name="isInternal"/>
    <set inverse="true" name="planningGroups">
      <key column="planning_type_id"/>
      <one-to-many class="PlanningGroup"/>
    </set>
    <many-to-one class="Subdepartment" column="subdepartment_id" name="subdepartment" not-null="false"/>
  </class>
</hibernate-mapping>
