<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="StandardSellingRate" table="standard_selling_rate">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <many-to-one class="Position" name="position">
      <column name="position_id" not-null="true"/>
    </many-to-one>
    <property name="amount"/>
    <many-to-one class="StandardSellingRateGroup" name="standardSellingRateGroup">
      <column name="standard_selling_rate_group_id" not-null="true"/>
    </many-to-one>
  </class>
</hibernate-mapping>
