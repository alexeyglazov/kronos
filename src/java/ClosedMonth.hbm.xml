<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="ClosedMonth" table="closed_month">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="year" not-null="true"/>
    <property name="month" not-null="true"/>
    <many-to-one class="Country" column="country_id" name="country" not-null="true"/>
  </class>
</hibernate-mapping>
