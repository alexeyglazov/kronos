<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="AnnualPaidLeave" table="annual_paid_leave">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="start" column="startDate" not-null="true"/>
    <property name="end" column="endDate" />
    <property name="duration" not-null="true" />
    <property name="contractType" not-null="true" />
    <many-to-one class="Position" column="position_id" name="position" not-null="true"/>
  </class>
</hibernate-mapping>
