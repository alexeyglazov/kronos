<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="Salary" table="salary">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="start" column="startDate" not-null="true"/>
    <property name="end" column="endDate" />
    <!--<property name="value" not-null="true"/>-->
    <property name="encipheredValue" not-null="true"/>
    <many-to-one class="Currency" name="currency">
      <column name="currency_id" not-null="true"/>
    </many-to-one>
    <many-to-one class="Employee" name="employee">
      <column name="employee_id" not-null="true"/>
    </many-to-one>
  </class>
</hibernate-mapping>
