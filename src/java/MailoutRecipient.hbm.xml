<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="MailoutRecipient" table="mailout_recipient">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="status"/>
    <property name="source"/>
    <property name="email"/>
    <property name="comment"/>
    <many-to-one class="Mailout" name="mailout">
      <column name="mailout_id" not-null="true"/>
    </many-to-one>
    <many-to-one class="Contact" name="contact">
      <column name="contact_id" not-null="false"/>
    </many-to-one>
    <many-to-one class="Employee" name="employee">
      <column name="employee_id" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
