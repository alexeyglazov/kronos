<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="ProjectCodeConflict" table="project_code_conflict">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="status" not-null="true"/>
    <many-to-one class="Employee" column="modified_by_id" name="modifiedBy" not-null="true"/>
    <property name="modifiedAt" not-null="true"/>  
    <many-to-one class="ProjectCode" column="project_code_id" name="projectCode" not-null="true"/>
    <many-to-one class="Subdepartment" column="checking_subdepartment_id" name="checkingSubdepartment" not-null="true"/>
    <set inverse="true" name="projectCodeConflictHistoryItems">
      <key column="project_code_conflict_id"/>
      <one-to-many class="ProjectCodeConflictHistoryItem"/>
    </set>
  </class>
</hibernate-mapping>
