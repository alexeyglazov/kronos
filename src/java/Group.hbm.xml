<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="Group" table="client_group">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="alias" not-null="false"/>
    <many-to-one column="country_id" name="country" not-null="false"/>
    <property name="isListed"/>
    <property name="isReferred"/>
    <property name="isMazarsAudit"/>    
    <many-to-one column="listing_country_id" name="listingCountry" not-null="false"/>
    <set inverse="true" name="clients">
      <key>
        <column name="group_id" not-null="true"/>
      </key>
      <one-to-many class="Client"/>
    </set>
    <set inverse="true" name="clientHistoryItems">
      <key>
        <column name="group_id" not-null="true"/>
      </key>
      <one-to-many class="ClientHistoryItem"/>
    </set>
    <set inverse="true" name="groupHistoryItems">
      <key>
        <column name="group_id" not-null="true"/>
      </key>
      <one-to-many class="GroupHistoryItem"/>
    </set>
    <many-to-one column="work_country_id" name="workCountry" not-null="true"/>
  </class>
</hibernate-mapping>
