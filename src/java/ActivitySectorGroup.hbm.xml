<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="ActivitySectorGroup" table="activity_sector_group">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="description" length="2048"/>
    <set inverse="true" name="activitySectors">
      <key>
        <column name="activity_sector_group_id" not-null="true"/>
      </key>
      <one-to-many class="ActivitySector"/>
    </set>
  </class>
</hibernate-mapping>
