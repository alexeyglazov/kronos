<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="EmployeeSubdepartmentHistoryItem" table="employee_subdepartment_history_item">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="start" column="startDate" />
    <property name="end" column="endDate" />
    <property name="type" not-null="true"/>
    <many-to-one class="Employee" column="employee_id" name="employee" not-null="true"/>
    <many-to-one class="Subdepartment" column="subdepartment_id" name="subdepartment" not-null="true"/>
  </class>
</hibernate-mapping>
