<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="BusinessTripItem" table="business_trip_item">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="day" not-null="true" index="day_index"/>
    <property name="modifiedAt" not-null="true"/>
    <many-to-one class="Employee" column="employee_id" name="employee" not-null="true"/>
    <many-to-one class="ProjectCode" column="project_code_id" name="projectCode" not-null="false"/>
  </class>
</hibernate-mapping>
