<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="Employee" table="employee">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="userName"/>
    <!--<property name="password"/>-->
    <property name="hashedPassword" not-null="true"/>
    <property name="salt" not-null="true"/>
    <property name="passwordToBeChanged" not-null="true"/>
    <property name="firstName"/>
    <property name="lastName"/>
    <property name="firstNameLocalLanguage"/>
    <property name="lastNameLocalLanguage"/>
    <property name="maidenName"/>
    <property name="email"/>
    <property name="externalId"/>
    <property name="profile"/>
    <property name="isAdministrator" not-null="true"/>
    <property name="isActive"/>
    <property name="isFuture"/>
    <property name="secretKey"/>
    <property name="secretKeyDate"/>
    <set inverse="true" name="timeSpentItems">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="TimeSpentItem"/>
    </set>
    <set inverse="true" name="businessTripItems">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="BusinessTripItem"/>
    </set>
    <set inverse="true" name="employeePositionHistoryItems">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="EmployeePositionHistoryItem"/>
    </set>
    <set inverse="true" name="employeeContactLinks">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="EmployeeContactLink"/>
    </set>
    
    <set inverse="true" name="employeeSubdepartmentHistoryItems">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="EmployeeSubdepartmentHistoryItem"/>
    </set>

    <set inverse="true" name="employeeProjectCodeAccessItems">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="EmployeeProjectCodeAccessItem"/>
    </set>    
    <set inverse="true" name="createdProjectCodes">
      <key>
        <column name="created_by_id" not-null="true"/>
      </key>
      <one-to-many class="ProjectCode"/>
    </set>
    <set inverse="true" name="closedProjectCodes">
      <key>
        <column name="closed_by_id" not-null="true"/>
      </key>
      <one-to-many class="ProjectCode"/>
    </set>
    <set inverse="true" name="inChargeProjectCodes">
      <key>
        <column name="in_charge_person_id" not-null="true"/>
      </key>
      <one-to-many class="ProjectCode"/>
    </set>
    <set inverse="true" name="inChargePartnerProjectCodes">
      <key>
        <column name="in_charge_partner_id" not-null="true"/>
      </key>
      <one-to-many class="ProjectCode"/>
    </set>
    
    <many-to-one class="NaturalPerson" name="naturalPerson">
      <column name="natural_person_id" not-null="false"/>
    </many-to-one>
    
    <set inverse="true" name="rightsItems">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="RightsItem"/>
    </set>
    <set inverse="true" name="leavesItems">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="LeavesItem"/>
    </set>
    <set inverse="true" name="salaries">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="Salary"/>
    </set>
    <set inverse="true" name="clientHistoryItems">
      <key>
        <column name="created_by_id" not-null="true"/>
      </key>
      <one-to-many class="ClientHistoryItem"/>
    </set>
    <set inverse="true" name="contactHistoryItems">
      <key>
        <column name="modified_by_id" not-null="true"/>
      </key>
      <one-to-many class="ContactHistoryItem"/>
    </set>
    <set inverse="true" name="planningItems">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="PlanningItem"/>
    </set>    
    <set inverse="true" name="inChargePlanningGroups">
      <key>
        <column name="in_charge_person_id" not-null="true"/>
      </key>
      <one-to-many class="PlanningGroup"/>
    </set>     
    <many-to-one class="Position" column="position_id" name="position" not-null="true"/>

    <set inverse="true" name="mailoutHistoryItems">
      <key>
        <column name="employee_id" not-null="true"/>
      </key>
      <one-to-many class="MailoutHistoryItem"/>
    </set>
            
    <set inverse="true" name="mailoutRecipients">
      <key>
        <column name="employee_id" not-null="false"/>
      </key>
      <one-to-many class="MailoutRecipient"/>
    </set>    
  </class>
</hibernate-mapping>
