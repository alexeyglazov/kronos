<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="PlanningItem" table="planning_item">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="startDate" not-null="true"/>
    <property name="endDate" not-null="true"/>
    <property name="description"/>
    <property name="location"/>
    <many-to-one class="PlanningGroup" column="planning_group_id" name="planningGroup" not-null="false"/>
    <many-to-one class="Subdepartment" column="target_subdepartment_id" name="targetSubdepartment" not-null="true"/>
    <many-to-one class="Subdepartment" column="source_subdepartment_id" name="sourceSubdepartment" not-null="true"/>
    <many-to-one class="Employee" column="employee_id" name="employee" not-null="true"/>
  </class>
</hibernate-mapping>
