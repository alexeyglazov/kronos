<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="ContentItem" table="content_item">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="title"/>
    <property name="description"/>
    <property name="url"/>
    <property name="isNonUserAccessible"/>
    <property name="isUserAccessible"/>
    <property name="isSuperUserAccessible"/>
    <property name="isCountryAdministratorAccessible"/>
    <many-to-one class="ContentItem" name="parent">
      <column name="parent_id" not-null="false"/>
    </many-to-one>
    <many-to-one class="Module" name="module">
      <column name="module_id" not-null="false"/>
    </many-to-one>
  </class>
</hibernate-mapping>
