<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="FeesItem" table="fees_item">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="date"/>
    <property name="type" not-null="true"/>
    <many-to-one class="Currency" column="fees_advance_currency_id" name="feesAdvanceCurrency" not-null="true"/>
    <many-to-one class="Currency" column="fees_invoice_currency_id" name="feesInvoiceCurrency" not-null="true"/>
    <many-to-one class="Currency" column="fees_payment_currency_id" name="feesPaymentCurrency" not-null="true"/>
    <many-to-one class="Currency" column="fees_act_currency_id" name="feesActCurrency" not-null="true"/>
    <set inverse="true" name="positionQuotations">
      <key>
        <column name="fees_item_id" not-null="true"/>
      </key>
      <one-to-many class="PositionQuotation"/>
    </set>
    <property name="quotationCurrencyRate" not-null="false"/>
    <property name="quotationNegociated" not-null="false"/>
    <property name="vat"/>
    <property name="comment"/>
    <many-to-one column="project_code_id" name="projectCode" not-null="true" unique="true"/>
    <set inverse="true" name="feesAdvances">
      <key>
        <column name="fees_item_id" not-null="true"/>
      </key>
      <one-to-many class="FeesAdvance"/>
    </set>
    <set inverse="true" name="feesInvoices">
      <key>
        <column name="fees_item_id" not-null="true"/>
      </key>
      <one-to-many class="FeesInvoice"/>
    </set>
    <set inverse="true" name="feesPayments">
      <key>
        <column name="fees_item_id" not-null="true"/>
      </key>
      <one-to-many class="FeesPayment"/>
    </set>
    <set inverse="true" name="feesActs">
      <key>
        <column name="fees_item_id" not-null="true"/>
      </key>
      <one-to-many class="FeesAct"/>
    </set>
  </class>
</hibernate-mapping>
