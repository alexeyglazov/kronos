<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="Agreement" table="agreement">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="number" not-null="true"/>
    <property name="isSigned" not-null="true"/>
    <property name="date" not-null="true"/>
    <property name="type" not-null="true"/>
    <property name="isRenewal" not-null="true"/>
    <property name="comment" not-null="true"/>
    <many-to-one column="project_code_id" name="projectCode" not-null="true" unique="true"/>
  </class>
</hibernate-mapping>
