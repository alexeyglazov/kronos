<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="StandardCost" table="standard_cost">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <many-to-one class="Position" name="position">
      <column name="position_id" not-null="true"/>
    </many-to-one>
    <property name="amount"/>
    <many-to-one class="StandardCostGroup" name="standardCostGroup">
      <column name="standard_cost_group_id" not-null="true"/>
    </many-to-one>
  </class>
</hibernate-mapping>
