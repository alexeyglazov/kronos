<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="InvoiceRequestPacketHistoryItem" table="invoice_request_packet_history_item">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="status"/>
    <property name="comment"/>
    <property name="time"/>
    <many-to-one class="Employee" column="employee_id" name="employee" not-null="true"/>
    <many-to-one class="InvoiceRequestPacket" name="invoiceRequestPacket">
      <column name="invoice_request_packet_id" not-null="true"/>
    </many-to-one>
  </class>
</hibernate-mapping>
