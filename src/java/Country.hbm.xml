<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="Country" table="country">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="description"/>
    <set inverse="true" name="offices">
      <key>
        <column name="country_id" not-null="true"/>
      </key>
      <one-to-many class="Office"/>
    </set>
    <set inverse="true" name="closedMonths">
      <key column="country_id"/>
      <one-to-many class="ClosedMonth"/>
    </set>
    <set inverse="true" name="countryCurrencies">
      <key column="country_id"/>
      <one-to-many class="CountryCurrency"/>
    </set>
    <set inverse="true" name="groups">
      <key column="work_country_id"/>
      <one-to-many class="Group"/>
    </set>
    <set inverse="true" name="clients">
      <key column="work_country_id"/>
      <one-to-many class="Client"/>
    </set>    
  </class>
</hibernate-mapping>
