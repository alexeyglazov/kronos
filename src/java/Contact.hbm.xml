<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="Contact" table="contact">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="gender"/>
    <property name="firstName"/>
    <property name="lastName"/>
    <property name="firstNameLocalLanguage"/>
    <property name="lastNameLocalLanguage"/>

    <property name="comment"/>
    <property name="directPhone"/>
    <property name="mobilePhone"/>
    <property name="email"/>
    <property name="language"/>
    <many-to-one class="ISOCountry" name="residencialCountry">
      <column name="residencial_country_id" not-null="false"/>
    </many-to-one>
    <property name="isClientsAddressUsed"/>
    <property name="street"/>
    <property name="zipCode"/>
    <property name="city"/>
    <many-to-one column="country_id" name="country"/>

    <property name="normalPosition"/>
    <property name="otherNormalPosition"/>
    <property name="presentType"/>
    <property name="isReminder"/>
    <property name="isNewsletters"/>
    <property name="isActive" not-null="true"/>
    <set inverse="true" name="employeeContactLinks">
      <key>
        <column name="contact_id" not-null="true"/>
      </key>
      <one-to-many class="EmployeeContactLink"/>
    </set>
    <set inverse="true" name="contactClientLinks">
      <key>
        <column name="contact_id" not-null="true"/>
      </key>
      <one-to-many class="ContactClientLink"/>
    </set>        
    <set inverse="true" name="contactHistoryItems" table="contact_history_item">
      <key>
        <column name="contact_id" not-null="true"/>
      </key>
      <one-to-many class="ContactHistoryItem"/>
    </set>
    <set inverse="true" name="mailoutRecipients">
      <key>
        <column name="contact_id" not-null="false"/>
      </key>
      <one-to-many class="MailoutRecipient"/>
    </set>        
  </class>
</hibernate-mapping>
