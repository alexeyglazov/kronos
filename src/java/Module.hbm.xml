<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="Module" table="module">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="description"/>
    <property name="isReport" not-null="true"/>
    <set inverse="true" name="rightsItems" table="rights_item">
      <key>
        <column name="module_id" not-null="true"/>
      </key>
      <one-to-many class="RightsItem"/>
    </set>
  </class>
</hibernate-mapping>
