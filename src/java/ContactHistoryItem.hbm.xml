<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="ContactHistoryItem" table="contact_history_item">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="status" not-null="true"/>
    <property name="comment" not-null="true"/>
    <many-to-one class="Contact" name="contact">
      <column name="contact_id" not-null="true"/>
    </many-to-one>
    <property name="modifiedAt" not-null="true"/>
    <many-to-one column="modified_by_id" name="modifiedBy" not-null="true"/>
  </class>
</hibernate-mapping>
