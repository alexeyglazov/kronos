<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="ClientActivitySectorLink" table="client_activity_sector_link">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="sortValue" not-null="true"/>
    <many-to-one class="ActivitySector" column="activity_sector_id" name="activitySector" not-null="true"/>
    <many-to-one class="Client" column="client_id" name="client" not-null="true"/>
  </class>
</hibernate-mapping>
