<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="OutOfPocketRequestHistoryItem" table="out_of_pocket_request_history_item">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="status" not-null="true"/>
    <property name="comment" not-null="true"/>
    <property name="time" not-null="true"/>
    <many-to-one class="Employee" column="employee_id" name="employee" not-null="true"/>
    <many-to-one class="OutOfPocketRequest" name="outOfPocketRequest">
      <column name="out_of_pocket_request_id" not-null="true"/>
    </many-to-one>
  </class>
</hibernate-mapping>
