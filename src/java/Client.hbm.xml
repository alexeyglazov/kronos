<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="Client" table="client">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="clientGroup"/>
    <property name="name"/>
    <property name="codeName"/>
    <property name="alias" not-null="false"/>
    <many-to-one column="country_id" name="country"/>

    <property name="legalStreet"/>
    <property name="legalZipCode"/>
    <property name="legalCity"/>
    <many-to-one column="legal_country_id" name="legalCountry"/>

    <property name="isPostalAddressEqualToLegal"/>
    <property name="postalStreet"/>
    <property name="postalZipCode"/>
    <property name="postalCity"/>
    <many-to-one column="postal_country_id" name="postalCountry"/>

    <property name="phone"/>
    <property name="email"/>
    <property name="taxNumber"/>
    <property name="isListed"/>
    <many-to-one column="listing_country_id" name="listingCountry"/>
    <property name="isReferred"/>
    <property name="customerType"/>
    <property name="channelType"/>
    <property name="isActive"/>
    <property name="isFuture"/>
    <property name="isExternal"/>
    <property name="isTransnational"/>
    <property name="color" not-null="false"/>
    <many-to-one class="Group" name="group">
      <column name="group_id" not-null="false"/>
    </many-to-one>
    <set inverse="true" name="subdepartmentClientLinks">
      <key>
        <column name="client_id" not-null="true"/>
      </key>
      <one-to-many class="SubdepartmentClientLink"/>
    </set>  
    <set inverse="true" name="contactClientLinks">
      <key>
        <column name="client_id" not-null="true"/>
      </key>
      <one-to-many class="ContactClientLink"/>
    </set>      
    <set inverse="true" name="clientActivitySectorLinks">
      <key>
        <column name="client_id" not-null="true"/>
      </key>
      <one-to-many class="ClientActivitySectorLink"/>
    </set>      
    <set inverse="true" name="clientHistoryItems" table="client_history_items">
      <key>
        <column name="client_id" not-null="true"/>
      </key>
      <one-to-many class="ClientHistoryItem"/>
    </set>
    <set inverse="true" name="projectCodes" table="project_code">
      <key>
        <column name="client_id" not-null="true"/>
      </key>
      <one-to-many class="ProjectCode"/>
    </set>
    <set inverse="true" name="invoiceRequests">
      <key>
        <column name="client_id" not-null="true"/>
      </key>
      <one-to-many class="InvoiceRequest"/>
    </set>
    <set inverse="true" name="actRequests">
      <key>
        <column name="client_id" not-null="true"/>
      </key>
      <one-to-many class="ActRequest"/>
    </set>
    <set inverse="true" name="planningGroups">
      <key>
        <column name="client_id" not-null="false"/>
      </key>
      <one-to-many class="PlanningGroup"/>
    </set>
    <many-to-one column="work_country_id" name="workCountry" not-null="true"/>     
  </class>
</hibernate-mapping>
