<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="Activity" table="activity">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="codeName"/>
    <property name="isActive"/>
    <property name="isConflictCheck" not-null="true"/>
    <many-to-one class="Subdepartment" name="subdepartment">
      <column name="subdepartment_id" not-null="true"/>
    </many-to-one>
    <set inverse="true" name="projectCodes" table="project_code">
      <key>
        <column name="activity_id" not-null="true"/>
      </key>
      <one-to-many class="ProjectCode"/>
    </set>
  </class>
</hibernate-mapping>
