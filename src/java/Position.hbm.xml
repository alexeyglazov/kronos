<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="Position" table="position">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="localLanguageName"/>
    <property name="visitCardName"/>
    <property name="localLanguageVisitCardName"/>
    <property name="sortValue" not-null="true"/>
    <property name="isActive" not-null="true"/>
    <set inverse="true" name="employeePositionHistoryItems" table="employee_position_history_item">
      <key>
        <column name="position_id" not-null="true"/>
      </key>
      <one-to-many class="EmployeePositionHistoryItem"/>
    </set>
    <set inverse="true" name="annualPaidLeaves">
      <key>
        <column name="position_id" not-null="true"/>
      </key>
      <one-to-many class="AnnualPaidLeave"/>
    </set>
    <set inverse="true" name="employees" table="employee">
      <key>
        <column name="position_id" not-null="true"/>
      </key>
      <one-to-many class="Employee"/>
    </set>
    <set inverse="true" name="standardSellingRates">
      <key>
        <column name="position_id" not-null="true"/>
      </key>
      <one-to-many class="StandardSellingRate"/>
    </set>
    <set inverse="true" name="standardCosts">
      <key>
        <column name="position_id" not-null="true"/>
      </key>
      <one-to-many class="StandardCost"/>
    </set>
    <set inverse="true" name="positionQuotations">
      <key>
        <column name="position_id" not-null="true"/>
      </key>
      <one-to-many class="PositionQuotation"/>
    </set>
    <many-to-one class="Subdepartment" column="subdepartment_id" name="subdepartment" not-null="true"/>
    <many-to-one class="StandardPosition" column="standard_position_id" name="standardPosition" not-null="true"/>
  </class>
</hibernate-mapping>
