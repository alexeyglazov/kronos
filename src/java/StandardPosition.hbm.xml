<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="StandardPosition" table="standard_position">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="sortValue" not-null="true"/>
    <set inverse="true" name="positions">
      <key>
        <column name="standard_position_id" not-null="true"/>
      </key>
      <one-to-many class="Position"/>
    </set>
  </class>
</hibernate-mapping>
