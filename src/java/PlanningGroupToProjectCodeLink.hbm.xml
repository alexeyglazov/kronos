<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="PlanningGroupToProjectCodeLink" table="planning_group_to_project_code_link">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <many-to-one class="PlanningGroup" column="planning_group_id" name="planningGroup" not-null="true"/>
    <many-to-one class="ProjectCode" column="project_code_id" name="projectCode" not-null="true"/>
  </class>
</hibernate-mapping>
