<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mazars.management.db.domain">
  <class name="TaxInvoiceRequest" table="tax_invoice_request">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="reference"/>
    <property name="isCancelled" not-null="true"/>
    <property name="isExternallyCancelled" not-null="true"/>

    <many-to-one class="InvoiceRequestPacket" name="invoiceRequestPacket">
      <column name="invoice_request_packet_id" not-null="true"/>
    </many-to-one>   
  </class>
</hibernate-mapping>
